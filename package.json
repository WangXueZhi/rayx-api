{
  "name": "@bm/api",
  "version": "1.1.0",
  "description": "以swagger导出的json文件作为数据源，自动化生成接口调用代码",
  "main": "index.js",
  "author": "Dave",
  "bin": {
    "bm-api": "./bin/index.js"
  },
  "scripts": {
    "cz": "cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "publish": "npm publish && conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "bundleDependencies": false,
  "dependencies": {
    "cash": "^0.8.0",
    "chalk": "^2.4.2",
    "commander": "^2.18.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "ora": "^4.0.3",
    "prepend-file": "^1.3.1",
    "progress": "^2.0.3",
    "prompts": "^2.3.1",
    "shelljs": "^0.8.3",
    "single-line-log": "^1.1.2",
    "commitizen": "^4.2.4"
  },
  "peerDependencies": {
    "@types/webpack-env": "^1.17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"git commit trigger husky pre-commit hook\"",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "deprecated": false,
  "homepage": "https://gitlab.bitmartpro.com/exchange/web-frontend/cli-api/-/blob/master/README.md",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@gitlab.bitmartpro.com:exchange/web-frontend/cli-api.git"
  }
}
